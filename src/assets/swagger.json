		"/SchedulerSignup": {
			"post": {
				"x-swagger-router-controller": "login",
				"operationId": "SchedulerSignup",
				"tags": [
					"/SchedulerSignup"
				],
				"description": "Creates a new scheduler",
				"parameters": [{
					"name": "schedulerinfo",
					"in": "body",
					"required": true,
					"schema": {
						"type": "object",
						"required": [
							"group",
							"email",
							"password"
						],
						"properties": {
							"group": {
								"type": "string",
								"example": "company"
							},
							"email": {
								"type": "string",
								"example": "gmail@gmail.com"
							},
							"password": {
								"type": "string",
								"example": "password"
							}
						}
					}
				}],
				"responses": {
					"default": {
						"description": "Invalid request.",
						"schema": {
							"$ref": "#/definitions/Error"
						}
					},
					"200": {
						"description": "Scheduler signup request successful.",
						"schema": {
							"$ref": "#/definitions/Success"
						}
					}
				}
			}
		},
